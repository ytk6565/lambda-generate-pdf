{
  "name": "@ytk6565/lambda-generate-pdf-function",
  "version": "1.0.0",
  "description": "Lambda generate pdf",
  "main": "dist/index.js",
  "scripts": {
    "typecheck": "tsc",
    "check": "biome check --write .",
    "check:ci": "biome ci .",
    "test": "vitest",
    "prebuild": "rm -rf dist",
    "build": "node esbuild.config.mjs",
    "dev": "node esbuild.config.mjs && dotenv -e local/.local.env dotenv -e local/.aws.env -e local/.chromium.env -- node local/run-generate-pdf/index.mjs"
  },
  "author": "",
  "license": "ISC",
  "type": "module",
  "dependencies": {
    "pdf-lib": "^1.17.1",
    "puppeteer-core": "^21.6.0"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.701.0",
    "@biomejs/biome": "^1.9.4",
    "@sparticuz/chromium": "^131.0.1",
    "@types/aws-lambda": "^8.10.146",
    "@types/uuid": "^10.0.0",
    "dotenv-cli": "^8.0.0",
    "esbuild": "^0.24.0",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  }
}
